{
  "extends": "airbnb",
  "installedESLint": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "import/resolver": "webpack",
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "new-cap": ["error", { "capIsNewExceptions": ["Router", "Moment"] }]
  }
}
